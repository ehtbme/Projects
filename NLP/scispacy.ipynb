    "import scispacy\n",
    "import spacy\n",
    "nlp = spacy.load(\"en_ner_bionlp13cg_md\")  # Or another SciSpacy model\n",
    "text = \"Mutations in the APP gene are associated with Alzheimer's disease.\"\n",
    "doc = nlp(text)\n",
    "\n",
    "for ent in doc.ents:\n",
    "    print(ent.text, ent.label_)\n"
